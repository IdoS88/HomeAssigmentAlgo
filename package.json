{
  "name": "home-assignment-algo",
  "version": "1.0.0",
  "type": "module",
  "description": "Ride assignment algorithm with greedy and min-cost flow strategies",
  "main": "dist/cli.js",
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc",
    "start": "node dist/cli.js",
    "test": "tsx --test --test-reporter=spec",
    "test:unit": "tsx --test --test-reporter=spec src/**/*.test.ts --exclude src/**/*.integration.test.ts src/**/*.performance.test.ts",
    "test:integration": "tsx --test --test-reporter=spec src/**/*.integration.test.ts",
    "test:performance": "tsx --test --test-reporter=spec src/**/*.performance.test.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance",
    "test:coverage": "tsx --test --test-reporter=spec --coverage",
    "test:watch": "tsx --test --test-reporter=spec --watch",
    "test:debug": "tsx --inspect-brk --test --test-reporter=spec",
    "gen:data": "tsx tools/generate-data.ts --seed tools/seed.md",
    "gen:data:osrm": "tsx tools/generate-data.ts --seed tools/seed.md --verbose --validate"
  },
  "dependencies": {
    "zod": "^3.22.4",
    "pino": "^8.17.2"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
